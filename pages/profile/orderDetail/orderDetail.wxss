/* 密码 */
.btn_pay{
  margin: 100rpx auto; width: 600rpx; height: 100rpx; line-height: 100rpx; border-radius: 100rpx;
  background-color: #d3a95a; color: #fff; font-size: 36rpx; text-align: center; 
}
/* 支付密码css start */
.bg_layer{
  position: fixed; left: 0; top: 0; bottom: 0; right: 0;
  background-color: rgba(0, 0, 0, 0.6); z-index: 9998;
}
.input_main{
    position: fixed; left: 0; bottom: 500rpx; width: 100%; height: 394rpx;
    background-color: #fff; z-index: 9999;
}
.input_title{
    width: 100%; height: 90rpx; line-height: 90rpx; text-align: center; 
    font-size: 32rpx; border-bottom: 1rpx solid #e2e2e2;
}
.input_back{
    position: absolute; left: 0; top: 0;
    width: 80rpx; height: 90rpx; display: flex; justify-content: center; align-items: center;
}
.input_back text{
  width: 20rpx;
  height: 20rpx;
  background-color: white;
  border: 1rpx solid #aaa;
  border-width: 5rpx 0 0 5rpx;
  transform: rotate(-45deg);
}

.input_tip{ 
   margin: 30rpx; 
   align-content: center;
   font-size: 18px; 
   color: #888; 
   }

/* 密码掩码模拟 */
.input_row{
  width: 690rpx; 
  margin: 0 auto;
   height: 98rpx; 
   position: relative;
  display: flex;
   align-items: center; 
   border: 1rpx solid #e2e2e2; 
   border-radius: 20rpx;
}
.input_row .pwd_item{
  flex: 1;
   display: flex; 
   align-items: center; 
   justify-content: center; 
  height: 100%; 
  border-right: 1rpx solid #e2e2e2; 
  position: relative;
}
.pwd_item:nth-last-of-type(1) { 
  border-right: 0;
   } 
.pwd_item text { 
  width: 30rpx; 
  height: 30rpx;
   border-radius: 30rpx; 
   background-color: #555;
}


.forget_pwd{ 
  float: right;
   margin: 30rpx; 
   width: 100rpx; 
   text-align: right; 
   font-size: 24rpx; 
   color: #ff7800;
}

/* 文本输入框位置: 设置到左边隐藏 */
.input_control { 
  position: relative;
   left: -300rpx; 
   bottom: 0;
    width: 100rpx; 
    height: 100rpx;
}